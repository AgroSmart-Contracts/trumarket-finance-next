NODE_ENV=development

# Application domain URL (used for server-side operations)
APP_DOMAIN=http://localhost:3000

# Git commit hash for versioning (optional, defaults to 'v0.0.0')
COMMIT_HASH=
LOG_LEVEL=debug

# Enable pretty formatted logs: 'true' | 'false'
PRETTY_LOGS=true

NEXT_PUBLIC_DATABASE_URL=

# EVM Chain ID (hex format)
NEXT_PUBLIC_EVM_CHAIN_ID=0x2105

# Investment Token Contract Address (ERC20 token address, e.g., USDC)
NEXT_PUBLIC_INVESTMENT_TOKEN_CONTRACT_ADDRESS=

# Investment Token Decimals (typically 6 for USDC, 18 for most ERC20 tokens)
NEXT_PUBLIC_INVESTMENT_TOKEN_DECIMALS=6

# Deals Manager Contract Address (smart contract address for managing deals)
NEXT_PUBLIC_DEALS_MANAGER_ADDRESS=
